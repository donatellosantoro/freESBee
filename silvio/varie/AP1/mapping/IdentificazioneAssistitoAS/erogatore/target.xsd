<?xml version="1.0" encoding="utf-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:NS="http://www.intemaweb.com/iron/datatypes" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://www.intemaweb.com/iron/datatypes">
	
	<xs:element name="risposta_RichiestaRispostaSincrona_IdentificazioneAssistito" type="NS:risposta_RichiestaRispostaSincrona_IdentificazioneAssistito_Type">
		<xs:annotation>
			<xs:documentation>Rappresenta i tipi definiti per un'operazione di risposta sincrona a supporto del servizio di IdentificazioneAssistito </xs:documentation>
		</xs:annotation>
	</xs:element>
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	
	<!--    *******************************************************      -->
	<!--             INIZIO DEFINIZIONE TIPI SEMPLICI                  -->
	<!--    *******************************************************      -->
	<xs:simpleType name="RiscontroAutoptico">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=con autopsia 2=senza autopsia </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Telefono">
		<xs:annotation>
			<xs:documentation>Numero telefonico validato da regex (Valori numerici,  -/+, lunghezza max 25 caratteri)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="25"/>
			<xs:whiteSpace value="collapse"/>
			<xs:pattern value="[0-9/+-]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceEccezione">
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="50"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MessaggioEccezione">
		<xs:restriction base="xs:string">
			<xs:minLength value="0"/>
			<xs:maxLength value="65535"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Asl">
		<xs:annotation>
			<xs:documentation>3 Caratteri alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
			<xs:pattern value="[0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodificaNomenclatore">
		<xs:annotation>
			<xs:documentation>Codice ad 1 carattere alfanumerico. Codificare con √¢__N√¢__ (maiuscola) se  si utilizzano le codifiche nazionali; 
codificare con la lettera alfabetica minuscola (per evitare sovrapposizioni) che identifica la Regione nell'ambito del Sistema Informativo Sanitario nazionale se si utilizzano codifiche regionali.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="1"/>
			<xs:pattern value="[Na-z]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceSSN">
		<xs:restriction base="xs:string"/>
	</xs:simpleType>
	<xs:simpleType name="Indirizzo">
		<xs:annotation>
			<xs:documentation>(Lunghezza massima 100 caratteri)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="100"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumeroScheda">
		<xs:annotation>
			<xs:documentation>8 Caratteri alfanumerici (Le prime due cifre identificano l√¢__anno, le rimanenti sei un progressivo all√¢__interno dell√¢__anno)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="8"/>
			<xs:pattern value="[0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="RegimeRicovero">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1 = Ricovero ordinario, 2 = Ricovero diurno</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Regione">
		<xs:annotation>
			<xs:documentation>Codice Alfanumerico di 3 caratteri</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
			<xs:pattern value="[0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TipoRichiesta">
		<xs:restriction base="xs:string">
			<xs:enumeration value="IdentificazioneAssistito"/>
			<xs:enumeration value="SegnalazioneRicovero"/>
			<xs:enumeration value="SegnalazioneDimissione"/>
			<xs:enumeration value="SegnalazioneFruizionePrestazioniAmbulatoriali"/>
			<xs:enumeration value="InformativaMediciDiBase"/>
			<xs:enumeration value="DatiClinicoAmministrativi"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="SistemaCodificaDiagnosi">
		<xs:annotation>
			<xs:documentation>Enumerazione: 3=ICD9-CM 1997 (Valore costante)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TipoRicovero">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1 = ricovero programmato, non urgente 2 = ricovero urgente
3 = ricovero per trattamento sanitario obbligatorio (TSO)
4 = ricovero programmato con preospedalizzazione</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Cognome">
		<xs:annotation>
			<xs:documentation>30 Caratteri tutti maiuscoli</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="30"/>
			<xs:pattern value="[A-Z' ]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Nome">
		<xs:annotation>
			<xs:documentation>20 Caratteri tutti maiuscoli</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="20"/>
			<xs:pattern value="[A-Z ]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Sesso">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=Maschio, 2=Femmina</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Email">
		<xs:annotation>
			<xs:documentation>Validato da regex (Lunghezza min 6 caratteri, max 50 caratteri)</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="6"/>
			<xs:maxLength value="50"/>
			<xs:pattern value="[a-zA-Z][\w\.-]*[a-zA-Z0-9]@[a-zA-Z0-9][\w\.-]*[a-zA-Z0-9]\.[a-zA-Z][a-zA-Z\.]*[a-zA-Z]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Comune">
		<xs:annotation>
			<xs:documentation>Codice Istat 6 Caratteri Alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="6"/>
			<xs:pattern value="[0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceSSR">
		<xs:annotation>
			<xs:documentation>Codice Sanitario Individuale di 16 caratteri alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumeroRicetta">
		<xs:annotation>
			<xs:documentation>Codice a 16 caratteri alfanumerici. Riportare il numero della ricetta (dal codice a barre della ricetta va escluso il carattere speciale posto all√¢__inizio di ciascuno dei due campi che compongono il numero della ricetta)
Nel caso di prestazioni ad accesso diretto o di pronto soccorso, riportare il numero progressivo del paziente o il numero del registro.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="16"/>
			<xs:pattern value="[a-zA-Z0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceFiscale">
		<xs:annotation>
			<xs:documentation>CodiceFiscale di 16 caratteri alfanumerici validato da Regex</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:pattern value="[A-Za-z]{6}[\d]{2}[A-Za-z][\d]{2}[A-Za-z][\d]{3}[A-Za-z]"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceRegionaleMedico">
		<xs:annotation>
			<xs:documentation>Codice di 16 caratteri alfanumerici.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="16"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="TraumatismiEIntossicazioni">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=infortunio sul lavoro,2=incidente in ambiente domestico, 3=incidente stradale, 
4=violenza altrui, 
5=autolesione o tentativo suicidio, 9=altro tipo di incidente</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Reparto">
		<xs:annotation>
			<xs:documentation>Codice di 4 caratteri alfanumerici: I primi 4 caratteri utilizzati per identificare l√¢__unit√?¬† operativa nel modello ministeriale HSP.12</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MotivoRicovero">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=intervento chirurgico di elezione, 2=prima diagnosi, 3=ricovero successivo con stessa diagnosi, 
9=altro</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceIstituto">
		<xs:annotation>
			<xs:documentation>Codice di 8 Caratteri alfanumerici; Codifica come Flussi ministeriali mod. HSP.11.BIS o HSP.11</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="DiagnosiDimissione">
		<xs:annotation>
			<xs:documentation>Codice di 5 caratteri alfanumerici allineato a sinistra e completato con spazi. </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:whiteSpace value="preserve"/>
			<xs:length value="5"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Intervento">
		<xs:annotation>
			<xs:documentation>Codice di 4 caratteri alfanumerici allineato a sinistra e completato con spazi</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="4"/>
			<xs:whiteSpace value="preserve"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ProvenienzaPaziente">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=paziente che accede all√¢__Istituto di cura senza proposta di ricovero formulata da un medico; 2=paziente inviato all√¢__Istituto di cura dal medico di base; 3=ricovero precedentemente programmato dallo stesso Istituto di cura; 4=paziente trasferito da un altro Istituto di ricovero e cura pubblico; 5=paziente trasferito da un altro Istituto di ricovero e cura privato accreditato; 6=paziente trasferito da un altro Istituto di ricovero e cura privato non accreditato; 7=paziente trasferito da altra modalit√?¬† di ricovero nello stesso istituto; 9=altro</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="7"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ModalitaDimissione">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=paziente deceduto 2=dimissione ordinaria al domicilio del paziente
3=dimissione ordinaria presso una RSA
4=dimissione al domicilio del paziente con attivazione di ospedalizzazione domiciliare
5=dimissione volontaria (da utilizzare anche nei casi in cui il paziente in ciclo di trattamento diurno non si sia ripresentato durante il ciclo programmato)
6=trasferito ad altro istituto di ricovero e cura, pubblico o privato per acuti
7=trasferimento ad altro regime di ricovero o ad altro tipo di attivit√?¬† di ricovero    nell'ambito dello stesso istituto
8=trasferimento ad un istituto pubblico o privato di riabilitazione
9=dimissione ordinaria con attivazione di assistenza domiciliare integrata </xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="7"/>
			<xs:enumeration value="8"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="PosizioneUtenteTicket">
		<xs:annotation>
			<xs:documentation>Enumerazione: Usare la seguente codifica: 1=esente totale; 2=non esente; 3=pagamento quota ricetta.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="NumeroGiorniDH">
		<xs:annotation>
			<xs:documentation>Codice di 3 caratteri numerici.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="MotivoRicoveroDH">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1=ricovero diurno diagnostico, 2=ricovero diurno chirurgico, 3=ricovero diurno terapeutico, 
4=ricovero diurno riabilitativo</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="OnereDegenza">
		<xs:annotation>
			<xs:documentation>Enumerazione: 1 = Ricovero a totale carico del SSN, 2 = Ricovero a prevalente carico del SSN con parte delle spese a carico del paziente (differenza alberghiera)
3 = Ricovero con successivo rimborso (totale o parziale) a carico del SSN
4 = Ricovero senza oneri a carico del SSN
5 = Ricovero a prevalente carico del SSN con parte delle spese a carico del paziente (libera professione)
6 = Ricovero a prevalente carico sel SSN con parte delle spese a carico del     paziente (differenza alberghiera e libera professione)
9 = altro</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
			<xs:enumeration value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceDrg">
		<xs:annotation>
			<xs:documentation>Codice DRG, sempre a 3 caratteri alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceErrore">
		<xs:restriction base="xs:string">
			<xs:length value="2"/>
			<xs:whiteSpace value="collapse"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ElementoErrato">
		<xs:restriction base="xs:token"/>
	</xs:simpleType>
	<xs:simpleType name="CodicePrestazione">
		<xs:annotation>
			<xs:documentation>Codice di 7 caratteri alfanumerici. Nel caso si utilizzi la codifica nazionale indicare il codice comprensivo dei punti separatori</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="7"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="QuantitaPrestazione">
		<xs:annotation>
			<xs:documentation>Valore numerico di 3 caratteri compreso tra 1 e 999. 1 di default.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:positiveInteger">
			<xs:minInclusive value="1"/>
			<xs:maxInclusive value="999"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IdOperatore">
		<xs:annotation>
			<xs:documentation>Codice di 10 caratteri alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:minLength value="1"/>
			<xs:maxLength value="10"/>
			<xs:pattern value="[a-zA-Z0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="IdComunicazione">
		<xs:annotation>
			<xs:documentation>Codice di 20 caratteri alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
			<xs:minLength value="1"/>
			<xs:pattern value="[A-Za-z0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CodiceStrutturaErogatrice">
		<xs:annotation>
			<xs:documentation>Codice di 6 caratteri alfanumerici.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ID">
		<xs:annotation>
			<xs:documentation>Identifivativo record identico per ogni blocco di prestazioni dalla riga 1 alla riga 99. Ha la stessa funzione del codice ricetta e deve essere univoco per l'anno di riferimento una volta unito ai dati obbligatori dell'erogatore e al progressivo riga.</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:maxLength value="20"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="StatoCivile">
		<xs:annotation>
			<xs:documentation>Enumerazione: pu√?¬≤ assumere valori da 1 a 6</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:enumeration value="1"/>
			<xs:enumeration value="2"/>
			<xs:enumeration value="3"/>
			<xs:enumeration value="4"/>
			<xs:enumeration value="5"/>
			<xs:enumeration value="6"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="Stato">
		<xs:annotation>
			<xs:documentation>3 Caratteri alfanumerici</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:string">
			<xs:length value="3"/>
			<xs:pattern value="[0-9]*"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ImportoBase">
		<xs:restriction base="xs:string">
			<xs:pattern value="[0-9]*,[0-9]{2}"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ImportoDegenza">
		<xs:restriction base="NS:ImportoBase">
			<xs:maxLength value="9"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ImportoTicket">
		<xs:restriction base="NS:ImportoBase">
			<xs:maxLength value="7"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="ImportoTotale">
		<xs:restriction base="NS:ImportoBase">
			<xs:maxLength value="8"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:simpleType name="CFValidatoMEF">
		<xs:restriction base="xs:boolean"/>
	</xs:simpleType>
	<xs:simpleType name="Xpath">
		<xs:annotation>
			<xs:documentation>Percorso Xpath.
																Sono ammessi i seguenti tipi di espressione:
																  Selector    ::=    Path ( '|' Path )*  
																  Path    ::=    ('.//')? Step ( '/' Step )*  
																  Step    ::=    '.' | NameTest  
																  NameTest    ::=    QName | '*' | NCName ':' '*'  
																  child:: 			
			</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:pattern value="(\.//)?(((child::)?((\i\c*:)?(\i\c*|\*)))|\.)(/(((child::)?((\i\c*:)?(\i\c*|\*)))|\.))*(\|(\.//)?(((child::)?((\i\c*:)?(\i\c*|\*)))|\.)(/(((child::)?((\i\c*:)?(\i\c*|\*)))|\.))*)*"/>
		</xs:restriction>
	</xs:simpleType>
	<!--    *******************************************************      -->
	<!--             INIZIO DEFINIZIONE TIPI COMPOSTI               -->
	<!--    *******************************************************      -->
	<xs:complexType name="ListaInterventi">
		<xs:sequence>
			<xs:element name="InterventoChirurgicoPrincipale" type="NS:Intervento">
				<xs:annotation>
					<xs:documentation>Codice dell'intervento chirurgico principale</xs:documentation>
