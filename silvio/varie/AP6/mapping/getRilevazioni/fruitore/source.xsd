							<xs:element name="rilevazione_has_servizio.servizio" type="xs:string"/>
							<xs:element name="rilevazione_has_servizioaccessorio.servizioaccessorio" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
				<xs:element name="datiCostanti">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="data_rilevazione" type="xs:string"/>
							<xs:element name="codistatregione" type="xs:string"/>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
</xs:schema>
