<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xs:element name="datiCompleti">
		<xs:complexType>
			<xs:sequence>
				<xs:element name="datiDB" maxOccurs="unbounded">
					<xs:complexType>
						<xs:sequence>
							<xs:element name="rilevazione.idrilevazione" type="xs:integer"/>
							<xs:element name="rilevazione.codimpianto" type="xs:integer"/>
							<xs:element name="rilevazione.datarilevazione" type="xs:date"/>
							<xs:element name="rilevazione.codistatregione" type="xs:string"/>
							<xs:element name="rilevazione.superifcie" type="xs:integer"/>
							<xs:element name="rilevazione.numero_addetti" type="xs:integer"/>
							<xs:element name="rilevazione.numero_dipendenti" type="xs:integer"/>
							<xs:element name="rilevazione.tipologia" type="xs:string"/>
							<xs:element name="rilevazione.att_commericale" type="xs:string"/>
							<xs:element name="rilevazione.autolavaggio" type="xs:string"/>
